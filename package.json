{
    "name": "conslog",
    "displayName": "Conslog",
    "description": "For a better console.log() experience.",
    "publisher": "mrchrmn",
    "version": "1.5.2",
    "engines": {
        "vscode": "^1.58.0"
    },
    "icon": "conslog_icon.png",
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "configurationDefaults": {
            "[javascript]": {
                "editor.suggest.snippetsPreventQuickSuggestions": false
            }
        },
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            }
        ],
        "keybindings": [
            {
                "key": "alt+shift+c",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - log selection"
                }
            },
            {
                "key": "alt+shift+,",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - log lines to last semicolon"
                }
            },
            {
                "key": "alt+shift+b",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - log lines to last closing bracket"
                }
            },
            {
                "key": "alt+shift+d",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - delete console.logs"
                }
            },
            {
                "key": "alt+shift+u",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - undo log selection"
                }
            },
            {
                "key": "alt+shift+l",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - log variable"
                }
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mrchrmn/conslog.git"
    }
}
