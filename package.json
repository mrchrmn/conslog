{
    "name": "conslog",
    "displayName": "Conslog",
    "description": "Wrap console.log() around selection.",
    "publisher": "mrchrmn",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            }
        ],
        "keybindings": [
            {
                "key": "alt+shift+c",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - log selection"
                }
            },
            {
                "key": "alt+shift+d",
                "command": "editor.action.insertSnippet",
                "when": "editorTextFocus",
                "args": {
                    "name": "Conslog - clear console.logs"
                }
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mrchrmn/conslog.git"
    }
}
